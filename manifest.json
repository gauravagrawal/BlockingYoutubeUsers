{
    "name": "My First Extension",
    "version": "1.0",
    "description": "The first extension that I made.", 
    "content_scripts": [ 
	{
	   "matches": [
	       "http://*/*"
	   ], 
	   "js": ["jquery.js", "UserBlocker.js"]
	}
    ],
    "browser_action": { 
	"default_icon": "icon.png",
	"popup": "background.html"
    }, 
    "permissions": [ 
	"tabs", "http://*/*"
	]
}
